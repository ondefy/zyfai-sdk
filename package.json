{
  "name": "@zyfai/sdk",
  "version": "0.2.10",
  "description": "TypeScript SDK for Zyfai Yield Optimization Engine - Deploy Safe smart wallets, manage session keys, and interact with DeFi protocols",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:json": "typedoc --json docs/api.json",
    "docs:dev": "cd website && pnpm start",
    "docs:build": "pnpm docs && cd website && pnpm build",
    "docs:serve": "cd website && pnpm serve"
  },
  "keywords": [
    "zyfai",
    "defi",
    "yield",
    "yield-optimization",
    "web3",
    "ethereum",
    "smart-wallet",
    "safe",
    "erc-4337",
    "account-abstraction",
    "session-keys",
    "arbitrum",
    "base",
    "typescript",
    "sdk"
  ],
  "author": "Zyfai <ayushgupta0610@gmail.com>",
  "license": "MIT",
  "homepage": "https://zyf.ai",
  "bugs": {
    "url": "https://github.com/ondefy/zyfai-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ondefy/zyfai-sdk.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@rhinestone/module-sdk": "^0.2.10",
    "axios": "^1.6.0",
    "permissionless": "^0.2.38",
    "siwe": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.3",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": false
    }
  }
}
